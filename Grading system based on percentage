
a=int(input("enter phy marks:"))
b=int(input("enter chem marks:"))
c=int(input("enter maths marks:"))
s=a+b+c
per=s*100/300
print(f"{per:.2f}%")
if (per >=90):
    print("A Grade")
elif (per>=80):
     print("B Grade")
elif (per>=70):
    print("C Grade")
elif (per>=60):
     print("D grade")
else:
    print("FAIL")
